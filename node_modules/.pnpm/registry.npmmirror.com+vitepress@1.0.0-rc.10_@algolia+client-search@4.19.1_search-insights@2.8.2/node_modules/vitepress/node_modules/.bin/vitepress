#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/long/Desktop/vite/node_modules/.pnpm/registry.npmmirror.com+vitepress@1.0.0-rc.10_@algolia+client-search@4.19.1_search-insights@2.8.2/node_modules/vitepress/bin/node_modules:/Users/long/Desktop/vite/node_modules/.pnpm/registry.npmmirror.com+vitepress@1.0.0-rc.10_@algolia+client-search@4.19.1_search-insights@2.8.2/node_modules/vitepress/node_modules:/Users/long/Desktop/vite/node_modules/.pnpm/registry.npmmirror.com+vitepress@1.0.0-rc.10_@algolia+client-search@4.19.1_search-insights@2.8.2/node_modules:/Users/long/Desktop/vite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/long/Desktop/vite/node_modules/.pnpm/registry.npmmirror.com+vitepress@1.0.0-rc.10_@algolia+client-search@4.19.1_search-insights@2.8.2/node_modules/vitepress/bin/node_modules:/Users/long/Desktop/vite/node_modules/.pnpm/registry.npmmirror.com+vitepress@1.0.0-rc.10_@algolia+client-search@4.19.1_search-insights@2.8.2/node_modules/vitepress/node_modules:/Users/long/Desktop/vite/node_modules/.pnpm/registry.npmmirror.com+vitepress@1.0.0-rc.10_@algolia+client-search@4.19.1_search-insights@2.8.2/node_modules:/Users/long/Desktop/vite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
